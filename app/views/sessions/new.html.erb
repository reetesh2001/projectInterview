<h1 class="login">LogIn</h1>
<div class="container">
  <%= form_with url: login_path, local: true do |form| %>
    <div>
      <%= form.label :email %>
      <%= form.email_field :email %>
    </div>
    <div>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </div>
    <div>
      <%= form.submit "Login" %>
    </div>
  <% end %>
</div>
